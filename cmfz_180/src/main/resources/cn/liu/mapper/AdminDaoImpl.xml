<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.liu.dao.AdminDao">
    <select id="selectUserName" resultType="cn.liu.entity.Admin">
        select * from admin where username = #{UserName}
    </select>
    <select id="getSelect" resultType="cn.liu.entity.Admin">
        select * from admin
    </select>


</mapper>


